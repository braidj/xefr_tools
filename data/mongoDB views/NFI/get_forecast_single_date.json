/* Get the current forecast date */
[
    {
        $match: {
        schemaId: $id{Metrics Forecast Date}
        }
    }
    ,
    {
        $addFields: {
            forecast:$id{Metrics Forecast Date.Forecast Date From},
        }
    }
    ,
    {
        $sort: {
            forecast: -1,
        }
    }
    ,
    {
        $limit: 1
    }
    ,
    {
        $set: {
            mydate: "$forecast"
        }
    }
]
/* End */